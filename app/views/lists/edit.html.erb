<div class="well">
  <h3>Edit list</h3>

  <%= form_for @list, method: :patch do |f| %>

    <p>
      <%= f.label :title %><br>
      <%= f.text_field :title, class: "form-control" %>
    </p>

    <p>
      <%= f.label :description %><br>
      <%= f.text_area :description, class: "form-control" %>
    </p>

    <p>
      <%= f.submit %>
    </p>

  <% end %>
</div>