<div class="well">
  <h3>Edit Item</h3>
  <%= form_for [@item.list,@item] , method: :patch do |f| %>

    <p>
      <%= f.label :title %><br>
      <%= f.text_field :title, class: "form-control" %>
    </p>

    <p>
      <%= f.label :description %><br>
      <%= f.text_area :description, class: "form-control" %>
    </p>

    <p>
      <%= f.submit "save"%>
    </p>

  <% end %>

</div>