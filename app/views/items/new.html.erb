<%= form_for @list , url:list_items_path, html: { multipart: true } do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>
  <%= f.file_field :avatar%>
  <p>
    <%= f.submit "Add Item" %>
  </p>
<% end %>




