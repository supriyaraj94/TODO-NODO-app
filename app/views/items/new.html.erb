<%= form_for :item , url:list_items_path(@list), html: { multipart: true } do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>

  <p>
    <%= f.label "Choose an image" %><br>  
    <%= f.file_field :avatar%>
  </p>

  <p>
    <%= f.submit "Add Item" %>
  </p>
<% end %>




